
year <- sub("-.*", "", meta$Date)
note <- sprintf("R package version %s", meta$Version)

bibentry(bibtype = "Manual",
  title        = "{ManlyMix}: An {R} Package for Model-Based Clustering with {M}anly Mixture Models",
  author       = c(person("Xuwen Zhu"), person("Volodymyr Melnykov")),
  year         = year,
  note         = note,
  url          = "")

textVersion  = paste("Xuwen Zhu, Volodymyr Melnykov (", year, ")", "ManlyMix: An R Package for Model-Based Clustering with Manly Mixture Models.", note, ".")



